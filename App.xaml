<Application x:Class="SNT2_WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SNT2_WPF"
             StartupUri="View/MainViews/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/ResourcesDictionary/UIColors.xaml"/>

                <ResourceDictionary Source="/Styles/ResourcesDictionary/DefaultStyleMode.xaml"/>
                <ResourceDictionary Source="/Styles/ResourcesDictionary/DarkStyleMode.xaml"/>

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
